<h1>Edit Listings</h1>

<p><%= link_to "Back", @listing %></p>

<%= form_for @listing do |f| %>
  <p>
    <%= f.label :address %><br>
    <%= f.text_field :address %>
  </p>
  <p>
    <%= f.label :agent %><br>
    <%= f.text_field :agent %>
  </p>
  <p>
    <%= f.label :url %><br>
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>