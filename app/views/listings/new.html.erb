<h1>Create New Listing</h1>

<%= form_tag "/listings" do %>
	<p>
		<%= label_tag :address %><br>
		<%= text_field_tag :address %>
	</p>

	<p>
		<%= label_tag :agent %><br>
		<%= text_field_tag :agent %>
	</p>

	<p>
		<%= label_tag :url %><br>
		<%= text_field_tag :url %>
	</p>

	<p>
		<%= submit_tag "Create Listing" %>
	</p>

	<% end %>

	

