<h1>Create New Listing</h1>
<div class="forms">

<%= link_to "Back to Listings", listings_url %>
<%= form_for @listing do |f| %>
  

  <p>
    <%= f.label :Listing_Address %><br>
    <%= f.text_field :address %>
  </p>
  <p>
    <%= f.label :agent_name %><br>
    <%= f.text_field :agent %>
  </p>

    <p>
    <%= f.label :agent_website %><br>
    <%= f.text_field :website %>
  </p>

    <p>
    <%= f.label :agent_email %><br>
    <%= f.text_field :email %>
  </p>

    <p>
    <%= f.label :agent_phone_number %><br>
    <%= f.text_field :number %>
  </p>

    <p>
    <%= f.label :price %><br>
    <%= f.text_field :price %>
  </p>

  <p>
    <%= f.label :bathrooms %><br>
    <%= f.text_field :bathrooms %>
  </p>

	  <p>
    <%= f.label :bedrooms %><br>
    <%= f.text_field :bedrooms %>
  </p>

    <p>
    <%= f.label :square_feet %><br>
    <%= f.text_field :sqft %>
  </p>

    <p>
    <%= f.label :description %><br>
    <%= f.text_area :description, :size => "50x10" %>
  </p>

   <p>
    <%= f.label :image %><br>
    <%= f.file_field :image %>
  </p>
  


  <p>
    <%= f.submit "Save" %>
  </p>
  </div>
<% end %>